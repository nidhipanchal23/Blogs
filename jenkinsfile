pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
                sh "git clone https://github.com/kishancs2020/TicketBookingServiceJunitTesting.git"
            }
        }
        stage('install') {
            steps {
                sh "mvn install -f Blogs"
            }
        }
        stage('test') {
            steps {
                sh "mvn test -f Blogs"
            }
        }
        stage('package') {
            steps {
                sh "mvn package -f Blogs"
            }
        }
    }
}
